pipeline {
  agent any
  stages {
    stage('Buzz Buzz') {
      steps {
        echo 'Bees  Buzz'
      }
    }

    stage('Buzz Test') {
      steps {
        echo 'Buzz Bees'
        echo 'Bees Buzz'
      }
    }

    stage('Bug ') {
      steps {
        echo 'Bug Found'
      }
    }

    stage('Test') {
      steps {
        echo 'QA Done'
      }
    }

  }
}